<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">


 <object name="object_buttons" meta_type="CMF Action Category">

  <object name="create_data_set" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Create HOME data set</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@home-page-dataset</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object name="debug" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Debug</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@debug</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>


</object>


</object>